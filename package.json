{
  "name": "farm-alamal",
  "version": "1.0.0",
  "description": "Farm Management System",
  "author": "Ahmed Elnagar",
  "main": "main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "dist": "npm run build && electron-builder --publish=never"
  },
  "build": {
    "appId": "com.farm.alamal",
    "productName": "Farm Al Amal",
    "files": [
      "dist/**/*",
      "main.cjs",
      "package.json"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "target": "nsis"
    }
  },
  "devDependencies": {
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "vite": "^5.4.21"
  },
  "dependencies": {
    "@google/generative-ai": "^0.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  }
}
