{
  "name": "farm-alamal",
  "version": "1.0.0",
  "description": "Farm Al Amal Desktop App",
  "author": "Ahmed Elnagar",
  "main": "main.cjs",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "dist": "npm run build && electron-builder --publish=never"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "@google/genai": "^0.7.0",
    "better-sqlite3": "^12.6.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.31",
    "recharts": "^2.12.0"
  },
  "devDependencies": {
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "build": {
    "appId": "com.farm.alamal",
    "productName": "Farm Al Amal",
    "files": [
      "dist/**/*",
      "main.cjs"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    }
  }
}
